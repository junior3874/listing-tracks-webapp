{
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "editor.formatOnSave": false,
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
    
}
    
    

